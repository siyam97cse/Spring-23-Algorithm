
#include <iostream>
using namespace std;

int bs(int array[], int x, int l, int r) {

  while (l <= r) {
    int mid = l + (r - l) / 2;

    if (array[mid] == x)
      return mid;

    if (array[mid] < x)
      l = mid + 1;

    else
      r = mid - 1;
  }

  return -1;
}

int main(void) {
     int a;
cin>>a;
int array[a];
for(int i=0;i<a;i++){
    cin>>array[i];
}
  int x;
  cout<<"What key you are looking for?"<<endl;
cin>>x;
  int n = sizeof(array) / sizeof(array[0]);
  int result = bs(array, x, 0, n - 1);
  if (result == -1)
    cout<<"Not found"<<endl;
  else
    cout<<"Element is found at index "<<result<<endl;
}
